{
  "name": "oraculo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Oráculo: Análise, limpeza e organização de repositórios",
  "author": "Italo C Lopes",
  "license": "MIT",
  "main": "./dist/cli.js",
  "bin": {
    "oraculo": "./dist/cli.js"
  },
  "exports": {
    ".": "./dist/cli.js"
  },
  "engines": {
    "node": ">=24.0.4"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node ./dist/cli.js",
    "diagnosticar": "node ./dist/cli.js diagnosticar",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --check \"**/*.{ts,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,md}\"",
    "coverage:enforce": "vitest run --coverage --coverage.enabled=true --coverage.thresholds='{\"lines\":90,\"statements\":90,\"branches\":88,\"functions\":90}'",
    "security:deps": "npm audit --production --audit-level=moderate || true",
    "security:lint": "npx eslint --no-error-on-unmatched-pattern \"src/**/*.ts\"",
    "prepare": "husky install",
    "test": "vitest run",
    "test:ui": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "cli",
    "limpeza",
    "arquitetura",
    "análise",
    "estrutura",
    "organização"
  ],
  "dependencies": {
    "@babel/parser": "^7.24.8",
    "@babel/traverse": "^7.24.8",
    "@babel/types": "^7.24.8",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "micromatch": "^4.0.8",
    "xxhashjs": "^0.2.2",
    "p-limit": "^4.0.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "vitest": {
    "coverage": {
      "exclude": [
        "file1.ts",
        "file2.ts",
        "scripts/**",
        "**/scripts/**",
        "fora-do-src.js",
        "tmp-cache-file.ts"
      ]
    }
  },
  "lint-staged": {
    "*.{ts,js,cts,mts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
