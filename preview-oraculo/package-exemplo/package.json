{
  "name": "preview-oraculo",
  "version": "0.1.0",
  "private": true,
  "description": "Pacote que  referencia preview-oraculo local para facilitar testes e builds isolados.",
  "type": "module",
  "engines": {
    "node": ">=24.0.4"
  },
  "dependencies": {
    "oraculo-preview": "file:./preview-oraculo"
  },
  "scripts": {
    "install:oraculo": "npm install --prefix ./preview-oraculo",
    "build:oraculo": "npm run build --prefix ./preview-oraculo",
    "start:oraculo": "npm run start --prefix ./preview-oraculo",
    "postinstall": "npm install --prefix ./preview-oraculo && npm run build --prefix ./preview-oraculo",
    "install": "npm install"
  }

  ,"bin": {
    "oraculo": "./bin/oraculo.cjs"
  }
}
