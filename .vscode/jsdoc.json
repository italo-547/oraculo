{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".js$|.ts$",
        "excludePattern": "(node_modules|docs|dist)"
    },
    "plugins": [],
    "opts": {
        "destination": "./docs/jsdoc",
        "recurse": true,
        "template": "default"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}